{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "classes": true
    },
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off"
  }
}
